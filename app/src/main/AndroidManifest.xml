<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frank.chart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.frank.chart.ChartsActivity"
            android:label="@string/title_activity_charts" >
        </activity>
        <activity
            android:name="com.frank.chart.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.frank.chart.SeekBarActivity"
            android:label="@string/title_activity_seek_bar" >
        </activity>
        <activity
            android:name="com.frank.chart.SpinnerActivity"
            android:label="@string/title_activity_spinner" >
        </activity>
        <activity
            android:name="com.frank.chart.MultiTouchActivity"
            android:label="@string/title_activity_multi_touch" >
        </activity>
        <activity
            android:name="com.frank.chart.GradientActivity"
            android:label="@string/title_activity_gradient" >
        </activity>
        <activity
            android:name="com.frank.chart.HBARScrollActivity"
            android:hardwareAccelerated="false"
            android:label="@string/title_activity_hbarscroll" >
        </activity>
        <activity
            android:name="com.frank.chart.HLNScrollActivity"
            android:hardwareAccelerated="false"
            android:label="@string/title_activity_hlnscroll" >

            <!-- 2014-9-30  pand   解决了scrollview超过450dp不显示的问题 -->
        </activity>
        <activity
            android:name="com.frank.chart.CircleChartActivity"
            android:label="@string/title_activity_circle_chart" >
        </activity>
        <activity
            android:name="com.frank.chart.GaugeChartActivity"
            android:label="@string/title_activity_gauge_chart" >
        </activity>
        <activity
            android:name="com.frank.chart.ClickChartsActivity"
            android:label="@string/title_activity_click_charts" >
        </activity>
        <activity
            android:name="com.frank.chart.DialChartActivity"
            android:label="@string/title_activity_dial_chart" >
        </activity>
        <activity
            android:name="com.frank.chart.DialChart2Activity"
            android:label="@string/title_activity_dial_chart2" >
        </activity>
        <activity
            android:name="com.frank.chart.DialChart3Activity"
            android:label="@string/title_activity_dial_chart3" >
        </activity>
        <activity
            android:name="com.frank.chart.DialChart4Activity"
            android:label="@string/title_activity_dial_chart4" >
        </activity>
        <activity
            android:name="com.frank.chart.DySpActivity"
            android:label="@string/title_activity_dy_sp" >
        </activity>
    </application>

</manifest>